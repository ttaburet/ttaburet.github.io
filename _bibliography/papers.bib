	doi = {10.1145/3369412.3395074},
	isbn = {9781450370509},
	keywords = {image processing pipeline, JPEG domain, covariance, digital image steganography, sensor noise},
	location = {Denver, CO, USA},
	numpages = {11},
	pages = {139--149},
	publisher = {Association for Computing Machinery},
	series = {IH&amp;MMSec '20},
	title = {JPEG Steganography and Synchronization of DCT Coefficients for a Given Development Pipeline},
