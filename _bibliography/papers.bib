	date-modified = {2020-11-17 02:14:15 +0100},
	doi = {10.1145/3335203.3335715},
	isbn = {9781450368216},
	keywords = {jpeg domain, digital image steganography, covariance, sensor noise, image processing pipeline},
	location = {Paris, France},
	numpages = {6},
	pages = {57--62},
	publisher = {Association for Computing Machinery},
	series = {IH&amp;MMSec'19},
	title = {Computing Dependencies between DCT Coefficients for Natural Steganography in JPEG Domain},
